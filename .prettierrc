{
  "printWidth": 80,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "semi": false,
  "requirePragma": false,
  "proseWrap": "preserve",
  "arrowParens": "avoid",
  "tabWidth": 2,
  "useTabs": false,
  "objectWrap": "preserve",
  "plugins": ["prettier-plugin-tailwindcss"]
}
